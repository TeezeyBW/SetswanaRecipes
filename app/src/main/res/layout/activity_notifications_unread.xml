<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsUnreadVM" type="com.tswanarecipe.app.modules.notificationsunread.data.viewmodel.NotificationsUnreadVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_9pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearHeader"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_gray_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTitle"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_81pxh"
                    android:layout_weight="0.89"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationsUnreadVM.notificationsUnreadModel.txtTitle}"
                    style="@style/txtPoppinssemibold18_2" tools:text="@string/lbl_notifications"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearNotificationsU"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTabs"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTabs"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@{notificationsUnreadVM.notificationsUnreadModel.txtTabs}"
                    style="@style/txtSolidRounded_1" tools:text="@string/lbl_all"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTabsOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@{notificationsUnreadVM.notificationsUnreadModel.txtTabsOne}"
                    style="@style/txtSolidRounded_1" tools:text="@string/lbl_read"/>
                <EditText
                    android:background="@drawable/rectangle_bg_teal_600_radius_10"
                    android:ellipsize="end"
                    android:hint="@string/lbl_unread"
                    android:id="@+id/etLanguage"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@={notificationsUnreadVM.notificationsUnreadModel.etLanguageValue}"
                    android:textColorHint="@color/white_A700"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFoodtitle"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notificationsUnreadVM.notificationsUnreadModel.txtFoodtitle}"
                style="@style/txtPoppinssemibold11_4" tools:text="@string/lbl_today"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListnewrecipealer"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="3" tools:listitem="@layout/row_listnewrecipealer2"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFoodtitleOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notificationsUnreadVM.notificationsUnreadModel.txtFoodtitleOne}"
                style="@style/txtPoppinssemibold11_4" tools:text="@string/lbl_yesterday"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListnewrecipealerTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="2" tools:listitem="@layout/row_listnewrecipealer_two2"/>
            <View
                android:background="@drawable/rectangle_bg_bluegray_100_75_radius_12"
                android:id="@+id/viewBg"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_92pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_315pxh"/>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameNavBar"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_3pxv" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStack"
                android:layout_height="@dimen/_120pxv" android:layout_width="match_parent">

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:backgroundTint="@color/teal_600"
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_163pxh"
                    android:layout_marginStart="@dimen/_163pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_48pxh"
                    app:srcCompat="@drawable/img_plus" app:tint="@color/white_A700"/>
                <FrameLayout android:id="@+id/frameStackbgOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_106pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBgOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_106pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bg"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_25pxv"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowvolume"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.51"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageVolume"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_volume"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                            <ImageView android:id="@+id/imageUnion"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginStart="@dimen/_45pxh"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_union"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowalarm"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_126pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.49"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageAlarm"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_17pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_alarm"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_alarm"/>
                            <ImageView android:id="@+id/imageSearch"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_48pxh"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search_bluegray_100"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search_bluegray_100"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
