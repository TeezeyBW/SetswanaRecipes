<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileVM" type="com.tswanarecipe.app.modules.profile.data.viewmodel.ProfileVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearColumn"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_9pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumntitle"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowtitle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTitle"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.55"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{profileVM.profileModel.txtTitle}"
                    style="@style/txtPoppinssemibold18_2" tools:text="@string/lbl_profile"/>
                <LinearLayout android:id="@+id/linearRowsettings"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_77pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_weight="0.45"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                    <ImageView android:id="@+id/imageUnion"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_union_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_union_black_900"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowlabel"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImageEighteen"
                    android:layout_height="@dimen/_99pxh"
                    android:layout_width="@dimen/_99pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_image18}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image18"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearRecipe"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.16"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLabel"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtLabel}"
                        style="@style/txtPoppinsregular11" tools:text="@string/lbl_recipe"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTitleOne"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtTitleOne}"
                        style="@style/txtPoppinssemibold20_1" tools:text="@string/lbl_4"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearFollowers"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_weight="0.21"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLabelTwo"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtLabelTwo}"
                        style="@style/txtPoppinsregular11" tools:text="@string/lbl_followers"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtDistance"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtDistance}"
                        style="@style/txtPoppinssemibold20_1" tools:text="@string/lbl_2_5m"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearFollowing"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_weight="0.21"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLabelOne"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtLabelOne}"
                        style="@style/txtPoppinsregular11" tools:text="@string/lbl_following"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtTwoHundredFiftyNine"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtTwoHundredFiftyNine}"
                        style="@style/txtPoppinssemibold20_1" tools:text="@string/lbl_259"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLabelTwo1"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileVM.profileModel.txtLabelTwo1}"
                style="@style/txtPoppinssemibold16_2" tools:text="@string/lbl_tetlo"/>
            <TextView android:ellipsize="end" android:id="@+id/txtBio"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileVM.profileModel.txtBio}"
                style="@style/txtPoppinsregular11" tools:text="@string/lbl_chef"/>
            <TextView android:gravity="start"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:text="@{profileVM.profileModel.txtDescription}"
                style="@style/txtPoppinsregular11_5" tools:text="@string/msg_private_chef_pa"/>
            <LinearLayout android:id="@+id/linearTabs"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowrecipe"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.9"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnRecipe"
                        android:layout_height="@dimen/_33pxv"
                        android:layout_width="@dimen/_107pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_recipe"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_recipe"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearTabs1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.27"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtLabelThree"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{profileVM.profileModel.txtLabelThree}"
                            style="@style/txtPoppinssemibold11_2" tools:text="@string/lbl_videos"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearTabs2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_62pxh"
                    android:layout_marginStart="@dimen/_83pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_weight="0.1"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLabelFour"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtLabelFour}"
                        style="@style/txtPoppinssemibold11_2" tools:text="@string/lbl_tag"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerProfile"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_20pxh}" tools:itemCount="3" tools:listitem="@layout/row_profile"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameNavBar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStack"
                android:layout_height="@dimen/_120pxv" android:layout_width="match_parent">

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:backgroundTint="@color/teal_600"
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_163pxh"
                    android:layout_marginStart="@dimen/_163pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_48pxh"
                    app:srcCompat="@drawable/img_plus" app:tint="@color/white_A700"/>
                <FrameLayout android:id="@+id/frameStackbg"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_106pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBg"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_106pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bg"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_25pxv"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowvolume"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageVolume"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_volume"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                            <ImageView android:id="@+id/imageUnionOne"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginStart="@dimen/_45pxh"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_union"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRownotification"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_125pxh"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageNotification"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_notification"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                            <ImageView android:id="@+id/imageUser"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_47pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_user_teal_50"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_user_teal_50"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
