<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingsVM" type="com.tswanarecipe.app.modules.settings.data.viewmodel.SettingsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnbg"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnlabel"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_5">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtyperegularfiftysix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumntyperegularfiftysix"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.37"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageImageEighteen"
                        android:layout_height="@dimen/_110pxh"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_image18}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image18"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTypeRegularFiftySix"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{settingsVM.settingsModel.txtTypeRegularFiftySix}"
                        style="@style/txtPoppinssemibold20_2" tools:text="@string/lbl_account"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnsettings"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_46pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_weight="0.63"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowsettings"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSettings"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.81"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{settingsVM.settingsModel.txtSettings}"
                            style="@style/txtPoppinssemibold18_2" tools:text="@string/lbl_settings"/>
                        <ImageView android:id="@+id/imageUnion"
                            android:layout_height="@dimen/_4pxh"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginStart="@dimen/_90pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_union_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_union_black_900"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUsername"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_50pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{settingsVM.settingsModel.txtUsername}"
                        style="@style/txtPoppinssemibold32" tools:text="@string/lbl_tetlo"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearBigButton"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabel"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_104pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.86"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingsVM.settingsModel.txtLabel}"
                    style="@style/txtPoppinssemibold16_3" tools:text="@string/lbl_edit_profile"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_93pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_teal_600"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_teal_600"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearBigButton1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabelOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_79pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingsVM.settingsModel.txtLabelOne}"
                    style="@style/txtPoppinssemibold16_3" tools:text="@string/lbl_app_permissions"/>
                <ImageView android:id="@+id/imageArrowrightOne"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_82pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_teal_600"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_teal_600"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTypeRegularFiftySixOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_46pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{settingsVM.settingsModel.txtTypeRegularFiftySixOne}"
                style="@style/txtPoppinssemibold20_2" tools:text="@string/lbl_app"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearBigButton2"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabelTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_weight="0.9"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingsVM.settingsModel.txtLabelTwo}"
                    style="@style/txtPoppinssemibold16_3" tools:text="@string/lbl_about_the_app"/>
                <ImageView android:id="@+id/imageArrowrightTwo"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_91pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_teal_600"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_teal_600"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearBigButton3"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLabelThree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_75pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_weight="0.92"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingsVM.settingsModel.txtLabelThree}"
                    style="@style/txtPoppinssemibold16_3" tools:text="@string/msg_rate_on_app_sto"/>
                <ImageView android:id="@+id/imageArrowrightThree"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_78pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_teal_600"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_teal_600"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnLogout"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_40pxv"
                android:layout_marginBottom="@dimen/_160pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_56pxv"
                android:layout_width="@dimen/_90pxh"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_21pxh"
                android:paddingStart="@dimen/_22pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_log_out"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/lbl_log_out"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameNavBar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStack"
                android:layout_height="@dimen/_120pxv" android:layout_width="match_parent">

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:backgroundTint="@color/teal_600"
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_163pxh"
                    android:layout_marginStart="@dimen/_163pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_48pxh"
                    app:srcCompat="@drawable/img_plus" app:tint="@color/white_A700"/>
                <FrameLayout android:id="@+id/frameStackbg"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_106pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBg"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_106pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bg"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_25pxv"
                        android:layout_marginEnd="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_32pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowvolume"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.47"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageVolume"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_volume"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                            <ImageView android:id="@+id/imageUnionOne"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginStart="@dimen/_45pxh"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_union"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRownotification"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_125pxh"
                            android:layout_weight="0.53"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageNotification"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_notification"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                            <ImageView android:id="@+id/imageSearch"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginStart="@dimen/_60pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search_bluegray_100"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search_bluegray_100"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
