<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editProfileVM" type="com.tswanarecipe.app.modules.editprofile.data.viewmodel.EditProfileVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearEditProfile"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:id="@+id/linearColumnsettings"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_5">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowsettings"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSettings"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtSettings}"
                    style="@style/txtPoppinssemibold18_2" tools:text="@string/lbl_edit_profile"/>
                <ImageView android:id="@+id/imageUnion"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_union_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_union_black_900"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackplus"
                android:layout_gravity="start"
                android:layout_height="@dimen/_116pxv"
                android:layout_marginEnd="@dimen/_113pxh"
                android:layout_marginStart="@dimen/_113pxh"
                android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_129pxh">
                <ImageView android:id="@+id/imageImageEighteen"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_110pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_110pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_image18}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image18"/>
                <ImageView android:id="@+id/imagePlus"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_plus_bluegray_401"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus_bluegray_401"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUsername"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtUsername}"
                style="@style/txtPoppinssemibold32" tools:text="@string/lbl_tetlo"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtYourEmail"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtYourEmail}"
                style="@style/txtPoppinsregular14" tools:text="@string/lbl_your_email"/>
            <EditText
                android:background="@drawable/rectangle_bg_bluegray_100_border_bluegray_100_radius_10"
                android:ellipsize="end"
                android:hint="@string/msg_teezeydagreat_g"
                android:id="@+id/etEmail"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={editProfileVM.editProfileModel.etEmailValue}"
                android:textColorHint="@color/gray_600"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end" android:id="@+id/txtLabel"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtLabel}"
                style="@style/txtPoppinsregular14" tools:text="@string/lbl_user_name"/>
            <EditText
                android:background="@drawable/rectangle_border_bluegray_100_radius_10"
                android:ellipsize="end" android:hint="@string/lbl_tetlo"
                android:id="@+id/etUserNameEdit"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_19pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@={editProfileVM.editProfileModel.etUserNameEditValue}"
                android:textColorHint="@color/bluegray_100"
                style="@style/etRoundedOutline" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLabelOne"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtLabelOne}"
                style="@style/txtPoppinsregular14" tools:text="@string/lbl_password"/>
            <EditText
                android:background="@drawable/rectangle_border_bluegray_100_radius_10"
                android:ellipsize="end" android:hint="@string/lbl"
                android:id="@+id/etPassword"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_21pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={editProfileVM.editProfileModel.etPasswordValue}"
                android:textColorHint="@color/bluegray_100"
                style="@style/etRoundedOutline" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSaveChanges"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="@dimen/_174pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/lbl_save_changes"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_save_changes"/>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameNavBar"
                android:layout_height="@dimen/_120pxv"
                android:layout_marginTop="@dimen/_30pxv" android:layout_width="match_parent">
                <View
                    android:background="@drawable/rectangle_bg_teal_600_radius_24"
                    android:id="@+id/viewBg"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_48pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_163pxh"
                    android:layout_marginStart="@dimen/_163pxh" android:layout_width="@dimen/_48pxh"/>
                <FrameLayout android:id="@+id/frameStackbgOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_106pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBgOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_106pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bg"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
                    <LinearLayout android:id="@+id/linearRowvolume"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_32pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVolume"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_volume"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                        <ImageView android:id="@+id/imageUnionOne"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_45pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_union"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginBottom="@dimen/_32pxv"
                            android:layout_marginStart="@dimen/_55pxh"
                            android:layout_width="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_white_a700"/>
                        <ImageView android:id="@+id/imageNotification"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_52pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_notification"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                        <ImageView android:id="@+id/imageSearch"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginStart="@dimen/_60pxh"
                            android:layout_marginTop="@dimen/_25pxv"
                            android:layout_width="@dimen/_13pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_search_bluegray_100"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_search_bluegray_100"/>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
