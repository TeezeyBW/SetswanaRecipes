<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeNavigationVM" type="com.tswanarecipe.app.modules.homenavigation.data.viewmodel.HomeNavigationVM"/>
    </data>
    <FrameLayout android:id="@+id/frameHomeNavigation"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_19pxv"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnhellotetlo"
            android:layout_gravity="top|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowhellotetlo"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearTitttle"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.79"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHelloTetlo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtHelloTetlo}"
                            style="@style/txtPoppinssemibold20" tools:text="@string/lbl_hello_tetlo"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtWhatareyouco"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtWhatareyouco}"
                            style="@style/txtPoppinsregular11" tools:text="@string/msg_what_are_you_co"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_21pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                    <FrameLayout android:id="@+id/frameProfileAvatar"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_40pxh" style="@style/groupStyleorange_200cornerRadius">
                        <ImageView android:id="@+id/imageRecipeImage"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_40pxh"
                            android:layout_width="@dimen/_40pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_d6cbc6849fe48a0"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_d6cbc6849fe48a0"/>
                    </FrameLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowsearch"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_bg_white_a700_border_bluegray_100_radius_10"
                    android:id="@+id/searchViewSearch"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.86"
                    android:layout_width="0dp"
                    android:theme="@style/searchViewSearch"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="@string/lbl_search_recipe" app:searchIcon="@drawable/img_search"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_teal_600_radius_10"
                    android:id="@+id/btnCamera"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:paddingHorizontal="@dimen/_11pxh"
                    android:paddingVertical="@dimen/_11pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_camera" tools:ignore="ContentDescription"/>
            </LinearLayout>
            <HorizontalScrollView android:fillViewport="true"
                android:id="@+id/horizontalScrollHorizontalscroll"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent" android:scrollbars="none">
                <LinearLayout android:id="@+id/linearRowTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.23"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButton"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.17"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButton}"
                            style="@style/txtSolidRounded_2" tools:text="@string/lbl_all"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonOne"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.24"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonOne}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_lunch"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonTwo"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.26"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonTwo}"
                            style="@style/txtSolidRounded_3" tools:text="@string/lbl_snacks"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonThree"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.32"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_5pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonThree}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_beverages"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_493pxh"
                        android:layout_marginStart="@dimen/_94pxh"
                        android:layout_weight="-0.23"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonFour"
                            android:layout_height="wrap_content"
                            android:layout_weight="-1.12"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonFour}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_fruit"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonFive"
                            android:layout_height="wrap_content"
                            android:layout_weight="-1.79"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonFive}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_vegetables"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonSix"
                            android:layout_height="wrap_content"
                            android:layout_weight="-1.38"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonSix}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_protein"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonSeven"
                            android:layout_height="wrap_content"
                            android:layout_weight="-1.33"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonSeven}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_cereal"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtButtonEight"
                            android:layout_height="wrap_content"
                            android:layout_weight="-1.88"
                            android:layout_width="0dp"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_20pxh"
                            android:paddingStart="@dimen/_20pxh"
                            android:paddingTop="@dimen/_7pxv"
                            android:singleLine="true"
                            android:text="@{homeNavigationVM.homeNavigationModel.txtButtonEight}"
                            style="@style/txtSolidRounded_1" tools:text="@string/lbl_local_dishes"/>
                    </LinearLayout>
                </LinearLayout>
            </HorizontalScrollView>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="end" android:id="@+id/recyclerCards"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_25pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_15pxh}" tools:itemCount="2" tools:listitem="@layout/row_cards2"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewRecipes"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{homeNavigationVM.homeNavigationModel.txtNewRecipes}"
                style="@style/txtPoppinssemibold16" tools:text="@string/lbl_new_recipes"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="end"
                android:id="@+id/recyclerCardsNewRecip"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_15pxh}" tools:itemCount="5" tools:listitem="@layout/row_cardsnewrecip"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameNavBar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="wrap_content">
            <FrameLayout android:id="@+id/frameStack"
                android:layout_height="@dimen/_120pxv" android:layout_width="match_parent">

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:backgroundTint="@color/teal_600"
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_163pxh"
                    android:layout_marginStart="@dimen/_163pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_48pxh"
                    app:srcCompat="@drawable/img_plus" app:tint="@color/white_A700"/>
                <FrameLayout android:id="@+id/frameStackbg"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_106pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBg"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_106pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bg"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
                    <LinearLayout android:id="@+id/linearRowThree"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_25pxv"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowvector"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageVector"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_vector"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                            <ImageView android:id="@+id/imageUnion"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginStart="@dimen/_45pxh"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_union"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRownotification"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_125pxh"
                            android:layout_weight="0.5"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageNotification"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_notification"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                            <ImageView android:id="@+id/imageSearchOne"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_47pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search_bluegray_100"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search_bluegray_100"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
