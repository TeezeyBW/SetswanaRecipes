<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsAllVM" type="com.tswanarecipe.app.modules.notificationsall.data.viewmodel.NotificationsAllVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumntitle"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_9pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearNotificationsA"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end" android:id="@+id/txtTitle"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notificationsAllVM.notificationsAllModel.txtTitle}"
                style="@style/txtPoppinssemibold18_2" tools:text="@string/lbl_notifications"/>
            <LinearLayout android:id="@+id/linearTabs"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowall"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.82"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnAll"
                        android:layout_height="@dimen/_33pxv"
                        android:layout_width="@dimen/_107pxh"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_all"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_all"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearTabs1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_46pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.21"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtLabel"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{notificationsAllVM.notificationsAllModel.txtLabel}"
                            style="@style/txtPoppinssemibold11_2" tools:text="@string/lbl_read"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearTabs2"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:layout_marginStart="@dimen/_79pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.18"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtLanguage"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{notificationsAllVM.notificationsAllModel.txtLanguage}"
                        style="@style/txtPoppinssemibold11_2" tools:text="@string/lbl_unread"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFoodtitle"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notificationsAllVM.notificationsAllModel.txtFoodtitle}"
                style="@style/txtPoppinssemibold11_4" tools:text="@string/lbl_today"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListnewrecipealer"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="3" tools:listitem="@layout/row_listnewrecipealer"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFoodtitleOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notificationsAllVM.notificationsAllModel.txtFoodtitleOne}"
                style="@style/txtPoppinssemibold11_4" tools:text="@string/lbl_yesterday"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListnewrecipealerTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="2" tools:listitem="@layout/row_listnewrecipealer_two"/>
            <View
                android:background="@drawable/rectangle_bg_bluegray_100_75_radius_12"
                android:id="@+id/viewBg"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_92pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_315pxh"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameNavBar"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_3pxv" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStack"
                android:layout_height="@dimen/_120pxv" android:layout_width="match_parent">

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:backgroundTint="@color/teal_600"
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_163pxh"
                    android:layout_marginStart="@dimen/_163pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_48pxh"
                    app:srcCompat="@drawable/img_plus" app:tint="@color/white_A700"/>
                <FrameLayout android:id="@+id/frameStackbgOne"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_106pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBgOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_106pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bg"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_25pxv"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowvolume"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.51"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageVolume"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_volume"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                            <ImageView android:id="@+id/imageUnion"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginStart="@dimen/_45pxh"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_union"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowalarm"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_126pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.49"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageAlarm"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_17pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_alarm"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_alarm"/>
                            <ImageView android:id="@+id/imageSearch"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_48pxh"
                                android:layout_width="@dimen/_13pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_search_bluegray_100"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_search_bluegray_100"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
