<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="recipeIngridentVM" type="com.tswanarecipe.app.modules.recipeingrident.data.viewmodel.RecipeIngridentVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_12pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearHeader"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_gray_900"/>
                <ImageView android:id="@+id/imageUnion"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_276pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_union_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_union_black_900"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRecipeIngriden"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameImage"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_150pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_315pxh" style="@style/groupStylecornerRadius_1">
                    <ImageView android:id="@+id/imageRecipeImage"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_150pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_10pxh}"
                        imageRes="@{@drawable/img_rectangle647}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle647"/>
                    <FrameLayout
                        android:id="@+id/frameStackrectangle648"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_150pxv" android:layout_width="@dimen/_315pxh">
                        <ImageView android:id="@+id/imageRecipePicture"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_150pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_10pxh}"
                            imageRes="@{@drawable/img_rectangle648}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle648"/>
                        <LinearLayout android:id="@+id/linearColumnstar"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearRating"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_268pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="wrap_content"
                                android:orientation="horizontal" style="@style/groupStyleorange_1">
                                <ImageView android:id="@+id/imageStar"
                                    android:layout_height="@dimen/_7pxh"
                                    android:layout_marginBottom="@dimen/_4pxv"
                                    android:layout_marginStart="@dimen/_7pxh"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_width="@dimen/_7pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_star"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtForty"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginEnd="@dimen/_7pxh"
                                    android:layout_marginStart="@dimen/_3pxh"
                                    android:layout_marginTop="@dimen/_2pxv"
                                    android:layout_weight="0.62"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{recipeIngridentVM.recipeIngridentModel.txtForty}"
                                    style="@style/txtPoppinsregular8" tools:text="@string/lbl_4_0"/>
                            </LinearLayout>
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearTime"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginEnd="@dimen/_7pxh"
                                android:layout_marginStart="@dimen/_215pxh"
                                android:layout_marginTop="@dimen/_90pxv"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <ImageView android:id="@+id/imageClock"
                                    android:layout_height="@dimen/_15pxh"
                                    android:layout_marginBottom="@dimen/_4pxv"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_width="@dimen/_13pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_clock_bluegray_100"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock_bluegray_100"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtTime"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_4pxv"
                                    android:layout_marginStart="@dimen/_6pxh"
                                    android:layout_marginTop="@dimen/_2pxv"
                                    android:layout_weight="0.5"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{recipeIngridentVM.recipeIngridentModel.txtTime}"
                                    style="@style/txtPoppinsregular11_2" tools:text="@string/lbl_20_min"/>
                                <ImageButton
                                    android:background="@drawable/rectangle_bg_white_a700_radius_12"
                                    android:id="@+id/btnComputer"
                                    android:layout_height="@dimen/_24pxh"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_width="@dimen/_24pxh"
                                    android:paddingHorizontal="@dimen/_5pxh"
                                    android:paddingVertical="@dimen/_5pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_computer" tools:ignore="ContentDescription"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearRowfoodtitle"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFoodtitle"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.62"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recipeIngridentVM.recipeIngridentModel.txtFoodtitle}"
                        style="@style/txtPoppinssemibold14_2" tools:text="@string/msg_quarter_and_atc"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTotalreviews"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_63pxh"
                        android:layout_weight="0.38"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recipeIngridentVM.recipeIngridentModel.txtTotalreviews}"
                        style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_13k_reviews"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearCreatorsProfi"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageUnsplashIj24Uq"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_unsplashij24uq_25x25}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_unsplashij24uq_25x25"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearInfo"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_weight="0.58"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLabel"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{recipeIngridentVM.recipeIngridentModel.txtLabel}"
                            style="@style/txtPoppinssemibold14_3" tools:text="@string/lbl_laura_wilson"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearLocation"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageLocation"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_12pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_location"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLabelOne"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{recipeIngridentVM.recipeIngridentModel.txtLabelOne}"
                                style="@style/txtPoppinsregular11" tools:text="@string/lbl_lagos_nigeria"/>
                        </LinearLayout>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnFollow"
                        android:layout_height="@dimen/_37pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_85pxh"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_24pxh"
                        android:paddingStart="@dimen/_24pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_follow"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_follow"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowingrident"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_A700">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnIngrident"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.72"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_8pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_8pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_ingrident"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_ingrident"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearTabs"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginEnd="@dimen/_75pxh"
                        android:layout_marginStart="@dimen/_61pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_weight="0.28"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtLabelTwo"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{recipeIngridentVM.recipeIngridentModel.txtLabelTwo}"
                            style="@style/txtPoppinssemibold11_2" tools:text="@string/lbl_procedure"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowitemscounter"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowgroup"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.57"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageGroup"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtServeCounter"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_weight="0.69"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{recipeIngridentVM.recipeIngridentModel.txtServeCounter}"
                            style="@style/txtPoppinsregular11" tools:text="@string/lbl_1_serve"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtItemsCounter"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_212pxh"
                        android:layout_weight="0.43"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{recipeIngridentVM.recipeIngridentModel.txtItemsCounter}"
                        style="@style/txtPoppinsregular11" tools:text="@string/lbl_10_items"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerIngridientList"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent"
                    android:nestedScrollingEnabled="false"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_10pxh}" tools:itemCount="14" tools:listitem="@layout/row_ingridientlist"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>
