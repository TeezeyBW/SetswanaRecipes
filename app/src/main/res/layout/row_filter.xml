<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="filterRowModel" type="com.tswanarecipe.app.modules.filter.data.model.FilterRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowcomponentoneOne"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <TextView android:ellipsize="end"
            android:id="@+id/txtComponentOneOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_weight="0.13" android:layout_width="0dp"
            android:paddingBottom="@dimen/_5pxv"
            android:paddingEnd="@dimen/_10pxh"
            android:paddingStart="@dimen/_10pxh"
            android:paddingTop="@dimen/_4pxv" android:singleLine="true"
            android:text="@{filterRowModel.txtComponentOneOne}"
            style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_all"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtComponentFourOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_weight="0.23" android:layout_width="0dp"
            android:paddingBottom="@dimen/_5pxv"
            android:paddingEnd="@dimen/_10pxh"
            android:paddingStart="@dimen/_10pxh"
            android:paddingTop="@dimen/_4pxv" android:singleLine="true"
            android:text="@{filterRowModel.txtComponentFourOne}"
            style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_cereal"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtComponentFive"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_weight="0.33" android:layout_width="0dp"
            android:paddingBottom="@dimen/_4pxv"
            android:paddingEnd="@dimen/_10pxh"
            android:paddingStart="@dimen/_10pxh"
            android:paddingTop="@dimen/_5pxv" android:singleLine="true"
            android:text="@{filterRowModel.txtComponentFive}"
            style="@style/txtSolidRoundedOutline_1" tools:text="@string/lbl_vegetables"/>
        <LinearLayout android:id="@+id/linearRowfive"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_weight="0.31" android:layout_width="0dp"
            android:orientation="horizontal" style="@style/groupStylewhite_7">
            <TextView android:ellipsize="end"
                android:id="@+id/txtFiveFive"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.67" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{filterRowModel.txtFiveFive}"
                style="@style/txtPoppinsregular11_7" tools:text="@string/lbl_dinner"/>
            <ImageView android:id="@+id/imageStarFiveFive"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star5_5"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star5_5"/>
        </LinearLayout>
    </LinearLayout>
</layout>
